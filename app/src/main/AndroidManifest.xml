<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.github.anotherjack.easyechodemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EchoMapActivity"
            android:label="回显Map数据"/>
        <activity android:name=".EchoBeanActivity"
            android:label="回显Java Bean数据"/>
        <activity android:name=".SaveAsBeanActivity"
            android:label="保存为Java Bean数据"/>
        <activity android:name=".SaveAsMapActivity"
            android:label="保存为Map数据"/>
    </application>

</manifest>